                {
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#D1BC92",
                    }
                },
                {
                    "scope": "keyword.other.special-method",
                    "settings": {
                        "foreground": "#44C0C6",
                    }
                },
                {
                    "scope": "variable.language",
                    "settings": {
                        "foreground": "#d79bff",
                    }
                },
                {
                    "scope": "constant.other.symbol",
                    "settings": {
                        "foreground": "#fa9672",
                    },
                },
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#bcc7d2",
                    },
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "foreground": "#D1BC92",
                    },
                },
                {
                    "scope": "source.elixir",
                    "settings": {
                        "foreground": "#bcc7d2",
                    },
                },
            ]
        },
